<%perl>

	my $semaphores = Baseliner->model('Baseliner::BaliSem')->search(undef, { prefetch=>['bali_sem_queues'], order_by=>'name ASC, bali_sem_queues.place DESC' });
	while( my $sem = $semaphores->next ) {
		my $queue = $sem->bali_sem_queues;
		for my $q ( $queue->all ) {
			print '<li>' . $q->sem_name->name . '=' . $q->place; 
		}
	}
</%perl>
